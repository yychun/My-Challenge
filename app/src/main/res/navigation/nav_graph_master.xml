<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_master"
    app:startDestination="@id/fragment_delivery_list">

    <fragment
        android:id="@+id/fragment_delivery_list"
        android:name="com.yychun1217.mychallenge.ui.DeliveryListFragment"
        android:label="@string/label_delivery_list"
        tools:layout="@layout/fragment_delivery_list">
        <action
            android:id="@+id/delivery_list_to_detail"
            app:destination="@+id/fragment_delivery_detail">
            <argument
                android:name="delivery"
                app:argType="com.yychun1217.mychallenge.model.Delivery$Ui"
                app:nullable="false" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/fragment_delivery_detail"
        android:name="com.yychun1217.mychallenge.ui.DeliveryDetailFragment"
        android:label="@string/label_delivery_detail"
        tools:layout="@layout/fragment_delivery_detail" />
</navigation>